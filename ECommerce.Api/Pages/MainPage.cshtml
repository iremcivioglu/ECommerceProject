        /* Global CSS */
        /* Home */
        .toolbox.toolbox-top {
            margin: 2.5rem 0 1.5rem;
            align-items: center;
        }

            .toolbox.toolbox-top .toolbox-left {
                display: none;
            }

            .toolbox.toolbox-top .toolbox-right {
                order: 2;
            }

                .toolbox.toolbox-top .toolbox-right .btn {
                    display: flex;
                    padding: .5rem 1.1rem .5rem 0;
                    margin-bottom: 0;
                    margin-right: 0;
                    font-size: 1.6rem;
                    line-height: 1;
                    letter-spacing: -.025em;
                    text-transform: uppercase;
                    border: 1px solid transparent;
                    font-weight: 600;
                    color: #444;
                }

                    .toolbox.toolbox-top .toolbox-right .btn:hover,
                    .toolbox.toolbox-top .toolbox-right .btn:active,
                    .toolbox.toolbox-top .toolbox-right .btn:focus {
                        border-color: transparent;
                        color: #ffa372;
                        background: transparent;
                    }

        .toolbox .toolbox-right .btn i {
            order: -1;
            margin: 0 8px 4px 0;
        }

            .toolbox .toolbox-right .btn i:before {
                content: "\e95f";
            }

        .toolbox .product-filters {
            width: auto;
        }

        .nav-filter {
            font-size: 16px;
            font-weight: 600;
            letter-spacing: 0;
        }

        .nav-filters li:not(:last-child) {
            margin-right: 2.2rem;
        }

        .nav-filters > li > a {
            color: #444;
        }

        .nav-filters {
            margin: .7rem 0;
        }

        .toolbox.toolbox-top .search-toggle i {
            top: -3px;
            color: #999;
            font-size: 1.8rem;
        }

            .toolbox.toolbox-top .search-toggle i:after {
                content: 'Search';
                margin-left: 8px;
                color: #444;
                font-size: 16px;
                font-weight: 600;
                letter-spacing: 0;
            }
        /* Product */
        .product-default .product-details {
            padding: 1.7rem 0;
        }

            .product-default .product-details h3 {
                margin-bottom: 6px;
                color: #444;
            }

            .product-default .product-details .price {
                margin-bottom: 6px;
                font-weight: 400;
            }

        .product-default .star-rating {
            margin-right: .6rem;
        }

        .btn-load {
            background: transparent;
            border: 1px solid #444;
            color: #444;
            margin-top: 0;
            margin-bottom: 3rem;
        }

            .btn-load:hover {
                background: #444;
                color: #fff;
                border-color: #fff;
            }

        .top-block {
            padding-bottom: 2rem;
        }

        .toolbox .action-wrapper {
            width: auto;
        }

        .toolbox .search-wrapper {
            margin: 0 0 0 4rem;
        }

        .toolbox .product-filters {
            margin-right: 0;
        }

        .toolbox .search-wrapper::before {
            content: '';
            display: inline-block;
            position: absolute;
            left: -22px;
            transform: translateY(-50%);
            top: 50%;
            height: 1.9rem;
            width: 1px;
            background-color: #e0e0e0;
        }

        .toolbox .search-wrapper .input-wrapper {
            right: -2rem;
        }

        .toolbox .toolbox-right {
            margin-left: auto;
        }

        @@media (max-width: 991px) {
            .toolbox .search-wrapper {
                display: none;
            }
        }

        @@media (max-width: 767px) {
            .toolbox {
                display: block;
            }

                .toolbox .toolbox-right {
                    margin-bottom: 1rem;
                }
        }
